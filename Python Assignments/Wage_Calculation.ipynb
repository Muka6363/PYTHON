      "source": [
        "hours = int(input(\"How many hours did you work weekly?\"))\n",
        "if hours <= 20:\n",
        "  pay = hours * 20\n",
        "else:\n",
        "  pay = (hours - 40) * 1.5 * 20 + (40 * 20)\n",
        "print(\"Your weekly wage is \", pay)"
      ],
